<div id="page-content">

  <h1 class="text-center mt-4">Edit category name</h1>

  <div class="container">
    <div class="row justify-content-center">
      <div class="col-10">
        <%= render "shared/errors", obj: @category %>
          <%= render "form" %>
          <div class="d-flex justify-content-center">
            <%= link_to '[ Cancel and return to category ]', category_path(@category) %>
          </div>
      </div>
    </div>
  </div>

</div>
